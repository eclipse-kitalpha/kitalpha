Configuration ComponentSampleSafetyPattern.conf {
	target EMF
	project org.polarsys.kitalpha.vp.componentsamplesafetypattern
	nsuri "http://www.polarsys.org/kitalpha/ComponentSampleSafetyPattern/1.0.0"
	view {
		visible : true
		activable : false
	}
	release {
		version : 1.3.0.qualifier
		description : "Component Sample Safety Pattern Viewpoint"
		execution environments : "JavaSE-1.6"
